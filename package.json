{
  "name": "gulp-start-html",
  "private": true,
  "version": "1.0.0",
  "description": "gulp-start HTML",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "^10.2.5",
    "browser-sync": "2.26.14",
    "gh-pages": "3.1.0",
    "gulp": "^4.0.2",
    "gulp-csso": "4.0.1",
    "gulp-htmlmin": "5.0.1",
    "gulp-imagemin": "4.1.0",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "2.0.0",
    "gulp-sass": "4.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "readable-stream": "3.6.0",
    "stylelint": "13.12.0",
    "stylelint-config-htmlacademy": "0.1.4"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "gulp"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/image/**/*.svg",
    "source/sass/**/*.scss"
  ],
  "engines": {
    "node": "14.15.3"
  },
  "dependencies": {
    "del": "^6.0.0"
  }
}
